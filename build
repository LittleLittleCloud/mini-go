ocamllex lexer.mll       # generates lexer.ml
ocamlyacc parser.mly     # generates parser.ml and parser.mli
ocamlc -c -g ast.ml
ocamlc -c -g VMType.ml 
ocamlc -c -g VM.ml 
ocamlc -c -g ICGType.ml
ocamlc -c -g ICG.ml
ocamlc -c -g pp_print_VMC.ml 
ocamlc -c -g VMC.ml 
ocamlc -c -g pp_ast.ml
ocamlc -c -g pp_fmt.ml
ocamlc -c -g parser.mli
ocamlc -c -g lexer.ml
ocamlc -c -g parser.ml
ocamlc -c -g typing.ml
ocamlc -c -g formatAST.ml
ocamlc -c -g pp_print_ICG.ml
ocamlc -c -g renameAST.ml
ocamlc -c -g pp.ml
ocamlc -g -o pp ast.cmo VMType.cmo VM.cmo renameAST.cmo ICGType.cmo ICG.cmo pp_print_VMC.cmo VMC.cmo pp_print_ICG.cmo pp_ast.cmo pp_fmt.cmo formatAST.cmo lexer.cmo parser.cmo pp.cmo
